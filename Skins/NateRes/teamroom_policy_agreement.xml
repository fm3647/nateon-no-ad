<Window size="300, 292" setround="0,0,0,0">
  <Font name="돋움" size="12" bold="false" />
  <Font name="돋움" size="12" bold="true"/>
  <Font name="돋움" size="11" bold="false" />
  <VerticalLayout bkcolor="#FFffffff" bordercolor="#FFAAAAAA" bordersize="1" inset="1,1,1,1">
    <!--타이틀, 닫기버튼-->
    <HorizontalLayout height="42">
      <Control width="20" />
      <!--타이틀-->
      <HorizontalLayout width="258" height="42" align="center">
        <VerticalLayout height="42">
          <Control height ="20" />
          <Label text="개인정보 수집 및 이용 동의" 
                 align="center" width="258" height="12" 
                 font="1" textcolor="#FF333333"
          />
          <Control height ="10" />
        </VerticalLayout>
      </HorizontalLayout>
      <!--닫기-->
      <VerticalLayout height="42" width="15" align="top">
        <Control height="5" />
        <Button name="policy_agree_close_btn"
                normalimage="btn_closepopup.png"
                hotimage="btn_closepopup_over.png"
                pushedimage="btn_closepopup_down.png"
                width="15" height="15"
          />
        <Control />
      </VerticalLayout>
      <Control width="6" />
    </HorizontalLayout>
    <!--경계선-->
    <HorizontalLayout height="1">
      <Control width="20" />
      <Control width="258" height="1" bordersize="1" bordercolor="#FFE9E9E9" />
      <Control width="20" />
    </HorizontalLayout>
    <Control height="15" />
    <HorizontalLayout>
      <Control width="20" />
      <!--약관 내용, 동의 체크박스 확인/취소 버튼-->
      <VerticalLayout height="206">
        <!--수집 목적-->
        <HorizontalLayout height="11">
          <Label text="수집목적" width="46" font="2" textcolor="#FF666666" />
          <Control width="5" />
          <Control bkimage="ico_more.png" width="3" height="9"/>
          <Control width="5" />
          <Label text="팀룸 서비스내 프로필 정보 제공" font="2" textcolor="#FF666666" />
        </HorizontalLayout>
        <Control height="7" />
        <!--수집항목-->
        <HorizontalLayout height="28">
          <VerticalLayout  height="11" width="46" align="top">
            <Label text="수집항목" height="11" width="46" font="2" textcolor="#FF666666" />
            <Control />
          </VerticalLayout>
          <Control width="5" />
          <VerticalLayout width="3">
            <Control bkimage="ico_more.png" width="3" height="9" />
            <Control />
          </VerticalLayout>
          <Control width="5" />
          <VerticalLayout>
            <Label text="직급/직책, 휴대폰번호, 전화번호, 이메일" font="2" textcolor="#FF666666" />
            <Label text=", 주요업무 중 이용자가 입력하는 정보" font="2" textcolor="#FF666666" />
          </VerticalLayout>
        </HorizontalLayout>
        <Control height="9" />
        <!--보유기간-->
        <HorizontalLayout height="11">
          <Label text="보유기간" width="46" font="2" textcolor="#FF666666" />
          <Control width="5" />
          <Control bkimage="ico_more.png" width="3" height="9" />
          <Control width="5" />
          <Label text="팀룸 탈퇴 및 회원 탈퇴 시 까지" font="2" textcolor="#FF666666" />
        </HorizontalLayout>
        <Control height="20" />
        <!--기타문구-->
        <VerticalLayout height="42">
          <Label text="동의를 거부하는 경우에도 팀룸서비스는 이용할 수 " font="2" textcolor="#FF999999" />
          <HorizontalLayout height="14">
            <Label text="있습니다. 다만, " font="2" width="80" textcolor="#FF999999" />
            <Label text="팀룸 내 프로필 수정은 불가합니다." font="2" textcolor="#FFff0000" />			
          </HorizontalLayout>
		  <Label text="입력하신 프로필 정보는 팀룸 멤버들에게 공개됩니다. " font="2" textcolor="#FF999999" />
        </VerticalLayout>
        <Control height="22" />
        <!--동의 체크박스-->    
        <HorizontalLayout height="15">
          <VerticalLayout width="15" align="top">
            <Button name="policy_agree_uncheck_btn"
                    normalimage="btn_uncheck_03.png"
                    hotimage="btn_uncheck_03_over.png"
                    pushedimage="btn_uncheck_03_down.png"
                    width="15" height="15"
            />
            <Button name="policy_agree_check_btn"
                    normalimage="btn_check_03.png"
                    hotimage="btn_check_03_over.png"
                    pushedimage="btn_check_03_down.png"
                    width="15" height="15"
                    visible="false"
                    enable ="false"
            />
            <Control />
          </VerticalLayout>
          <Control width="7" />
          <VerticalLayout height="15">
            <Label text="개인 정보 수집 및 이용에 동의합니다." font="0" textcolor="#FF333333" />
          </VerticalLayout>
        </HorizontalLayout>
        <Control height="20" />
        <!--확인/취소 버튼-->
        <HorizontalLayout height="22" align="center">
          <Control />
          <Button name="policy_agree_confirm_btn"
                  normalimage="btn_pop_confirm_s.png"
                  hotimage="btn_pop_confirm_s_over.png"
                  pushedimage="btn_pop_confirm_s_down.png"
                  width="40" height="22"
          />
          <Control width="10" />
          <Button name="policy_agree_cancel_btn"
                  normalimage="btn_pop_cancel_s.png"
                  hotimage="btn_pop_cancel_s_over.png"
                  pushedimage="btn_pop_cancel_s_down.png"
                  width="40" height="22"
          />
          <Control />
        </HorizontalLayout>
        <Control height="20" />
      </VerticalLayout>
      <Control width="15" />
    </HorizontalLayout>
  </VerticalLayout>
