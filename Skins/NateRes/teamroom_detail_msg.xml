<Window>
	<HorizontalLayout height="585" width="350">

		<VerticalLayout width="2" bkimage="troom_img_shade_detail.png" >
		</VerticalLayout>
		<VerticalLayout name="finder_msg_detail_layout">
			<HorizontalLayout height="45">
				<HorizontalLayout width="62" align="left">
					<Control width="7" />
					<VerticalLayout width="15">
						<Control />
						<Button name="detail_back" tooltip="상세 닫기"
										width="15" height="15"
										normalimage= "troom_bt_detailback.png"
										hotimage=	 "troom_bt_detailback_over.png"
										pushedimage= "troom_bt_detailback_down.png" />
						<Control />
					</VerticalLayout>
					<Control width="4" />
					<VerticalLayout width="36">
						<Control />
						<Label height="13"
								 text="메시지"
								 font="1"
								 textcolor="#FF222222" />
						<Control />
					</VerticalLayout>
					<Control />
				</HorizontalLayout>
				<Control />
				<HorizontalLayout width="35" align="right">
					<VerticalLayout width="25">
						<Control height="17" />
						<Button name="detail_close" tooltip="상세 닫기"
								width="25" height="15"
								normalimage= "troom_bt_detailclose.png"
								hotimage=	 "troom_bt_detailclose_over.png"
								pushedimage= "troom_bt_detailclose_down.png" />
						<Control height="13" />
					</VerticalLayout>
					<Control width="10" />
				</HorizontalLayout>
			</HorizontalLayout>
			<HorizontalLayout height="1">
				<Control bkcolor="#FFECECEC" />
			</HorizontalLayout>
			<List name="finder_msg_detail_list" inset="17, 0, 0, 10">
				<HorizontalLayout height="37">
					<VerticalLayout name="detail_msg_owner_width">
						<Control height="12" />
						<Label name="detail_msg_owner"
								 height="12"
								 text="장은지"
								 font="7"
								 textcolor="#FF333333"
								 endellipsis="true"	/>
						<Control height="13" />
					</VerticalLayout>
					<VerticalLayout width="60">
						<Control height="10" />
						<HorizontalLayout height="16">
							<HorizontalLayout width="19" height="16">
								<Button name="detail_msg_delete"
											 tooltip="삭제하기"
											 width="16" bkimage="troom_ic_delete.png" />
								<Control width="3" />
							</HorizontalLayout>
							<Button name="detail_msg_impo"
										 width="16" bkimage="troom_ic_flag_on.png" />
							<Control width="3" />
							<Button name="detail_msg_favo"
										 width="16" bkimage="troom_ic_scrap_on.png"	/>
							<Control width="9" />			 
						</HorizontalLayout>
						<Control />
					</VerticalLayout>
				</HorizontalLayout>
				<EllipsisText name="detail_msg"
									text=""
									lineinterval="3"
									textcolor="#FF333333"
									font="8" />
				<Control height="10" />
				<HorizontalLayout height="11">
					<VerticalLayout width="10">
						<Control />
						<Control height="5" bkimage="troom_bl_dot_01.png" />
						<Control />
					</VerticalLayout>
					<Label name="detail_msg_time"
								width="185"
								text="2015년 6월 28일 오후 22:12"
								textcolor="#FFB9B9B9"
								font="10"
								align="left"
								/>
					<Control />
				</HorizontalLayout>
				<Control height="9" />
				<HorizontalLayout height="11">
					<VerticalLayout width="10">
						<Control />
						<Control height="5" bkimage="troom_bl_dot_01.png" />
						<Control />
					</VerticalLayout>
					<Button name="detail_msg_preview"
								width="48"
								text="미리보기"
								tooltip="미리보기"
								textcolor="#FF9EBCC3"
								font="10"
								align="left" />
					<Control />
				</HorizontalLayout>
				<Control height="9" />
				<HorizontalLayout height="11">
					<VerticalLayout width="10">
						<Control />
						<Control height="5" bkimage="troom_bl_dot_01.png" />
						<Control />
					</VerticalLayout>
					<Label name="detail_msg_moim"
							 text="디자인팀모임인데 모임이름이 길경우"
							 textcolor="#FFB9B9B9"
							 font="10"
							 align="left"	/>
				</HorizontalLayout>
			</List>
		</VerticalLayout>
	</HorizontalLayout>
</Window>